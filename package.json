{
  "name": "generator-webpack-ts",
  "version": "4.3.0",
  "description": "A Yeoman Generator for creating websites with TypeScript and Webpack",
  "author": {
    "name": "Gary Bortosky",
    "email": "gary@bortosky.com",
    "url": "https://github.com/GaryB432"
  },
  "files": [
    "generators"
  ],
